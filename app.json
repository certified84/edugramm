{
  "expo": {
    "name": "edugramm",
    "slug": "edugramm",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/edugramm_splash.png",
      "backgroundColor": "#ffffff",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.certified.edugramm",
      "splash": {
        "image": "./assets/edugramm_splash.png",
        "backgroundColor": "#ffffff",
        "tabletImage": "./assets/edugramm_splash.png",
        "resizeMode": "cover"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.certified.edugramm",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to enable picture and video sharing and chatting.",
          "microphonePermission": "The app accesses your microphone to record audio"
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ce970083-4c34-48e4-a550-f50d12442f21"
      }
    }
  }
}
